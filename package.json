{
    "name": "ti-basic-syntax-highlighting",
    "displayName": "TI-Basic Syntax Highlighting",
    "description": "A syntax highlighter for Texas Instruments' TI-Basic Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tibasic",
            "aliases": ["TI-Basic", "tibasic"],
            "extensions": [".8XP",".8xp",".8XP.txt",".8XZ",".8xz"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tibasic",
            "scopeName": "source.8XP",
            "path": "./syntaxes/tibasic.tmLanguage.json"
        }]
    }
}